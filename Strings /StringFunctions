# String Functions

 #1. str.endswith("smnthg")
str = "i am a am student."
print(str.endswith("nt."))

 # 2. str.capitalize()
 
str= str.capitalize()
print(str)

# 3. str.replace(old,new)

print(str.replace("am","the")) 

# 4. str.find(word)

print(str.find("a"))

# 5. str.count(word)

print(str.count("am"))